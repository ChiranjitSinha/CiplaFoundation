<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="#003c64" />

            <corners android:radius="3dp" />

            <solid android:color="@color/login_btn_clicked" />

        </shape>

    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="#003c64" />

            <corners android:radius="3dp" />

            <solid android:color="@color/login_btn_unclicked" />

        </shape>
    </item>
</selector>

